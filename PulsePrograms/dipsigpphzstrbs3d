;dipsigpphzstrbs3d v0.0.1
;avance-version (10/12/07)
;2D homonuclear Hartman-Hahn transfer using 
;   DIPSI2 sequence for mixing
;   using selective refocussing with a shaped pulse
;with zero quantum suppression
;for band selective 2D generation
;H. Kessler, H. Oschkinat, C. Griesinger & W. Bermel,
;   J. Magn. Reson. 70, 106 (1986)
;J. Stonehouse, P. Adell, J. Keeler & A.J. Shaka, J. Am. Chem. Soc 116,
;   6037 (1994)
;K. Stott, J. Stonehouse, J. Keeler, T.L. Hwang & A.J. Shaka,
;   J. Am. Chem. Soc 117, 4199-4200 (1995)
;M.J. Thrippleton & J. Keeler,
;   Angew. Chem. Int. Ed. 42, 3938-3941 (2003)
;
;$CLASS=HighRes
;$DIM=2D
;$TYPE=
;$SUBTYPE=
;$COMMENT=


#include <Avance.incl>
#include <Grad.incl>
#include <Delay.incl>


;"FACTOR1=(d9/(p6*115.112))/2"
;"l1=FACTOR1*2"

"in0=inf1"
"d0=in0/2-p1*4/3.1416"

"spoff29=0"
"spoff30=0"



;#   ifdef CALC_SPOFFS
"spoff2=bf1*(cnst21/1000000)-o1"
;#   else
;#   endif /*CALC_SPOFFS*/


"acqt0=-p1*2/PI"
aqseq 312

1 ze
2 30m
3 d12 pl10:f1
  p17 ph27
  p17*2 ph28
  d12 pl1:f1 
  d1
  ;20u BLKGRAD
  50u UNBLKGRAD
  (p1 ph1):f1
  3u
  p16:gp1
  d16 pl0:f1
  p12:sp2:f1 ph2:r
  3u
  p16:gp1
  d16 pl1:f1
  d0
  p1 ph3
  10u gron0
  (p32:sp29 ph3):f1
  20u groff
  d16 pl10:f1


						;begin DIPSI2
4 p6*3.556 ph23
  p6*4.556 ph25
  p6*3.222 ph23
  p6*3.167 ph25
  p6*0.333 ph23
  p6*2.722 ph25
  p6*4.167 ph23
  p6*2.944 ph25
  p6*4.111 ph23
  
  p6*3.556 ph25
  p6*4.556 ph23
  p6*3.222 ph25
  p6*3.167 ph23
  p6*0.333 ph25
  p6*2.722 ph23
  p6*4.167 ph25
  p6*2.944 ph23
  p6*4.111 ph25

  p6*3.556 ph25
  p6*4.556 ph23
  p6*3.222 ph25
  p6*3.167 ph23
  p6*0.333 ph25
  p6*2.722 ph23
  p6*4.167 ph25
  p6*2.944 ph23
  p6*4.111 ph25

  p6*3.556 ph23
  p6*4.556 ph25
  p6*3.222 ph23
  p6*3.167 ph25
  p6*0.333 ph23
  p6*2.722 ph25
  p6*4.167 ph23
  p6*2.944 ph25
  p6*4.111 ph23
  lo to 4 times c
						;end DIPSI2

  p16:gp2
  d16
  10u gron3
  (p32*0.75:sp29 ph3):f1
  20u groff

  d16 pl1:f1
  4u BLKGRAD
  p1 ph3
  go=2 ph31
  30m mc #0 to 2 
      F1PH(calph(ph1, -90), caldel(d0, +in0))
      ;F2QF(ivc)
      F2QF(calclist(vc,1))
  ;lo to 1 times td2
  ;d12 ivc
  ;lo to 1 times td1
  ;d1 mc #0 to 2 F2PH(calph(ph1, +90), caldel(d0, +in0))
  ;d1 wr #0 if #0 ;F1PH(calph(ph1, +90), caldel(d0, +in0)) ; YDedit
  ;lo to 3 times 13; YDedit
  ;20u LOCKH_OFF
exit


ph1=0 2 
ph2=0 0 1 1 2 2 3 3
ph3=0
ph23=3
ph25=1
ph27=0
ph28=1
ph31=0 2 2 0


;pl0 : 0W
;pl1 : f1 channel - power level for pulse (default)
;pl10: CALCULATE - power level for TOCSY-spinlock
;sp2: CALCULATE - shaped pulse
;spnam2: Default Reburp.1000
;sp29: CALCULATE - shaped pulse (adiabatic)
;spnam29: DefaultCrp60,20,20.10
;p1 : DETERMINE -  90 degree high power pulse
;p6 : Spin lock 90 degree low power pulse [25µs]
;p12: sel 180 degree shaped pulse - selgpse [7.5ms]
;p16: homospoil/gradient pulse                       [1 msec]
;p17: trimming pulse    [2.5 msec]
;p32: Adiabatic 180 degree shaped pulse    [20 msec]
;     smoothed chirp (sweepwidth, 20% smoothing, 10000 points)
;p34: f1 channel - 180 degree shaped pulse (adiabatic)    [15 msec]
;     smoothed chirp (sweepwidth, 20% smoothing, 10000 points)
;d1 : relaxation delay; 1-5 * T1
;d9 : TOCSY mixing time
;d16: delay for homospoil/gradient recovery
;cnst21: chemical shift for selective pulse (offset, in ppm)
;l1: DISCONTINUED loop for DIPSI cycle: ((p6*115.112) * l1) = mixing time ;YDedit
;NS: 8 * n, total number of scans: NS * TD0 * 13
;DS: 4


;phcor 2 : phasedifference between power levels sp1 and pl1

;choose p12 according to desired selectivity
;the flip-angle is determined by the amplitude
;set O1 on resonance on the multiplet to be excited or use spoffs


;for z-only gradients:
;gpz0: ca. 11%
;gpz1: 15%
;gpz2: 31%

;use gradient files:
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100



;for sweepwidth of adiabatic shape and adjusting gpz0
;   see supplementary material of M.J. Thrippleton & J. Keeler,
;   Angew. Chem. Int. Ed. 42, 3938-3941 (2003)



;$Id: seldigpzs,v 1.2.2.1 2011/01/11 10:56:55 ber Exp $
